<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Aplikasi.ViewModel"
                    xmlns:view="clr-namespace:Aplikasi.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:KonsumenVM}">
        <view:Konsumen />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PemesananVM}">
        <view:Pemesanan />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProdukVM}">
        <view:Produk />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TransaksiVM}">
        <view:Transaksi />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingVM}">
        <view:Settings />
    </DataTemplate>



</ResourceDictionary>